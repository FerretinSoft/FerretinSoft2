<Window x:Class="pe.edu.pucp.ferretin.view.MSeguridad.MS_ReporteTrasaccionesParametrosWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Reportes Seguridad" Height="375" Width="525" Icon="../Resources/ferretin_ico.ico" ResizeMode="CanMinimize">
    <Grid>
        <Label Content="Parámetros" HorizontalAlignment="Left" Margin="28,23,0,0" VerticalAlignment="Top" FontSize="16" FontStyle="Italic"/>
        <Button x:Name="repSigBtn" Content="GENERAR" HorizontalAlignment="Right" Margin="0,245,55,0" Width="75" Height="31" Click="repSigBtn_Click" VerticalAlignment="Top"/>



        <DatePicker x:Name="finicialpicker" HorizontalAlignment="Left" Margin="206,108,0,0" VerticalAlignment="Top" Width="116" SelectedDateChanged="finicialpicker_SelectedDateChanged"/>
        <DatePicker x:Name="ffinalpicker" HorizontalAlignment="Left" Margin="206,158,0,0" VerticalAlignment="Top" Width="116" SelectedDateChanged="ffinalpicker_SelectedDateChanged"/>

        <Label Content="Fecha inicial:" HorizontalAlignment="Left" Margin="91,106,0,0" VerticalAlignment="Top" Width="82"/>
        <Label Content="Fecha final:" HorizontalAlignment="Left" Margin="91,157,0,0" VerticalAlignment="Top" Width="82"/>


    </Grid>
</Window>
