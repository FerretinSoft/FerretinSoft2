<Window x:Class="pe.edu.pucp.ferretin.view.MSeguridad.MS_ReporteTrasaccionesParametrosWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Reportes Seguridad" Height="375" Width="525" Icon="../Resources/ferretin_ico.ico" ResizeMode="CanMinimize">
    <Grid>
        <Label Content="Parámetros" HorizontalAlignment="Left" Margin="28,23,0,0" VerticalAlignment="Top" FontSize="16" FontStyle="Italic"/>
        <Button x:Name="repSigBtn" Content="GENERAR" HorizontalAlignment="Right" Margin="0,285,68.4,29.6" Width="75" Height="31" Click="repSigBtn_Click"/>
        <Button x:Name="repAntBtn" Content="&lt; ANTERIOR" HorizontalAlignment="Right" Margin="0,285,158.4,29.6" Width="75" IsEnabled="False" Height="31"/>



        <DatePicker x:Name="finicialpicker" HorizontalAlignment="Left" Margin="213,116,0,0" VerticalAlignment="Top" Width="116" SelectedDateChanged="finicialpicker_SelectedDateChanged"/>
        <DatePicker x:Name="ffinalpicker" HorizontalAlignment="Left" Margin="213,166,0,0" VerticalAlignment="Top" Width="116" SelectedDateChanged="ffinalpicker_SelectedDateChanged"/>

        <Label Content="Fecha inicial:" HorizontalAlignment="Left" Margin="98,114,0,0" VerticalAlignment="Top" Width="82"/>
        <Label Content="Fecha final:" HorizontalAlignment="Left" Margin="98,165,0,0" VerticalAlignment="Top" Width="82"/>


    </Grid>
</Window>
